{
  "name": "crypto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext=js --ext=jsx src",
    "eslint:fix": "eslint --ext=js --ext=jsx src --fix"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0"
  },
  "author": {
    "name": "huynhsamha",
    "email": "huynhsamha@gmail.com",
    "url": "https://github.com/huynhsamha"
  },
  "homepage": "https://github.com/huynhsamha/crypto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huynhsamha/crypto.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huynhsamha/crypto/issues"
  },
  "contributors": [
    "huynhsamha"
  ],
  "keywords": [
    "crypto"
  ]
}
